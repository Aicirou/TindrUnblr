{
  "name": "Tindr Unblr",
  "author": "Aicirou XO",
  "description": "Unblur Tinder likes!!",
  "version": "1.0.0",
  "manifest_version": 2,
  
  "background": {
    "scripts":["background.js"]
  },
  "content_scripts": [
    {
      "matches": ["http://tinder.com/app/likes-you", "https://tinder.com/app/likes-you"],
      "js": ["contentscript.js"]
    }
  ],
  "permissions": [
    "tabs"
  ]
}
