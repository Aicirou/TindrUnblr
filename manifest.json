{
  "name": "Tindr Unblr",
  "author": "Aicirou XO",
  "description": "Unblur Tinder likes!!",
  "version": "1.0.0",
  "manifest_version": 2,
  "background": {
    "scripts":["background.js"]
  },
  "icons": { 
    "256": "tindr256.png"
  },
  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*"],
      "js": ["contentscript.js"]
    }
  ],
  "permissions": [
    "tabs",
  	"http://tinder.com/app/likes-you",
    "https://tinder.com/app/likes-you"
  ]
}
